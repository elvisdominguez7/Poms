		<project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
		<java.version>1.8</java.version>
		<maven-site.version>3.7</maven-site.version>
		<maven-jar.version>3.0.2</maven-jar.version>
		<maven-javadoc.version>3.0.0</maven-javadoc.version>
		<maven-annot.version>3.5.1</maven-annot.version>
		<maven-failsafe.version>2.20.1</maven-failsafe.version>
		<maven-surefire.version>2.20.1</maven-surefire.version>
		<cxf.version>3.1.10</cxf.version>
		<cxf.jaxws.version>3.2.2</cxf.jaxws.version>
		<junit.version>4.12</junit.version>
		<jmockit.version>1.38</jmockit.version>
		<junit-tools.version>0.1.0</junit-tools.version>
		<pitest.version>1.2.0</pitest.version>
		<jacoco.version>0.8.0</jacoco.version>
		<findbug.version>3.0.4</findbug.version>
		<log4j.version>2.8.2</log4j.version>
		<log4j.version>2.10.0</log4j.version>
		<jackson.version>2.9.4</jackson.version>
		<joda.version>2.9.9</joda.version>
		<openjpa.version>2.4.2</openjpa.version>
