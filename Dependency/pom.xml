	<packaging>pom</packaging>

	<developers>
		<developer>
			<id>edominguez</id>
			<name>Elvis Dominguez</name>
			<email>elvisdominguez7@gmail.com</email>
			<organization>Edominguez Software, Inc.</organization>
			<url>https://github.com/elvisdominguez7</url>
			<roles>
				<role>Project lead</role>
			</roles>
		</developer>
	</developers>

	<scm>
		<url>https://github.com/elvisdominguez7/Dependency-Mgmt</url>
		<connection>scm:git:git://github.com/elvisdominguez7/Dependency-Mgmt.git</connection>
		<developerConnection>scm:git:git@github.com:elvisdominguez7/Dependency-Mgmt.git</developerConnection>
	</scm>

	<distributionManagement>
		<snapshotRepository>
			<id>edominguez</id>
			<url>http://localhost:8081/repository/edominguez/</url>
		</snapshotRepository>
	</distributionManagement>

	<repositories>
		<repository>
			<id>edominguez</id>
			<name>localhost Repository</name>
			<url>http://localhost:8081/repository/edominguez/</url>
			<releases>
				<enabled>true</enabled>
			</releases>
			<snapshots>
				<enabled>true</enabled>
			</snapshots>
		</repository>
	</repositories>

	<properties>
